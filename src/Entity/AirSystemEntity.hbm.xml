<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.AirSystemEntity" table="air_system" schema="dtsjwb">
        <id name="id" column="id"/>
        <property name="ecAirCon" column="ec_air_con"/>
        <property name="ecCuTube" column="ec_cu_tube"/>
        <property name="acAirCon" column="ac_air_con"/>
        <property name="acCuTube" column="ac_cu_tube"/>
        <property name="idcNumber" column="idc_number"/>
    </class>
</hibernate-mapping>