<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.RegisterUserEntity" table="register_user" schema="dtsjwb">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="pwd">
            <column name="pwd" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="identity">
            <column name="identity" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>