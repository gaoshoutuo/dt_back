<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.IdcInfoEntity" table="idc_info" schema="dtsjwb">
        <id name="id" column="id"/>
        <property name="idcId" column="idc_id"/>
        <property name="idcName" column="idc_name"/>
        <property name="idcType" column="idc_type"/>
        <property name="userId" column="user_id"/>
        <property name="userName" column="user_name"/>
        <property name="idcLocationDeep" column="idc_location_deep"/>
        <property name="idcLocationSimple" column="idc_location_simple"/>
    </class>
</hibernate-mapping>