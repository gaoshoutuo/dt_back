<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.EleSystemEntity" table="ele_system" schema="dtsjwb">
        <id name="id" column="id"/>
        <property name="frameUps" column="frame_ups"/>
        <property name="powerUnit" column="power_unit"/>
        <property name="battery" column="battery"/>
        <property name="idcNumber" column="idc_number"/>
    </class>
</hibernate-mapping>